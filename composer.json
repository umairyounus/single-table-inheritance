{
  "name": "nanigans/single-table-inheritance",
  "description": "Single Table Inheritance Trait",
  "keywords": ["single", "table", "inheritance", "laravel", "eloquent", "model"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jon Palmer",
      "email": "jpalmer@nanigans.com"
    }
  ],
  "require": {
    "php": ">= 5.5.9",
    "illuminate/database": "5.2.*|5.3.*|5.4.*|5.5.*",
    "illuminate/support": "5.2.*|5.3.*|5.4.*|5.5.*"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.8|~5.2",
    "orchestra/testbench": "3.2.*"
  },
  "autoload": {
    "psr-4": {
      "Nanigans\\SingleTableInheritance\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nanigans\\SingleTableInheritance\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
        "dev-master": "0.8.x-dev"
    }
  },
  "minimum-stability": "stable"
}
